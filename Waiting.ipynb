   "source": [
    "def waiting(count, ranges, _sleep = 0.01):\n",
    "    for i in trange(count, desc='1st loop'):\n",
    "        for j in tqdm(range(ranges), desc='2nd loop'):\n",
    "            sleep(_sleep)"
   ]
